  <% @aboutus.each do |aboutu| %>

    <div class="container-full-bg">
      <div class="container special">
        
        <div><br><br>
          <!-- Carousel ========================================== -->
          <div align="center" id="myProductCarousel" class="carousel slide carousel-left" data-interval="3500" data-ride="carousel">
            <!-- Indicators -->
            <ol class="carousel-indicators">
              <li data-target="#myProductCarousel" data-slide-to="0" class="active"></li>
              <li data-target="#myProductCarousel" data-slide-to="1"></li>
              <li data-target="#myProductCarousel" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner" role="listbox">
              <div class="item active">
                <img class="first-slide img_thumb_slide" src="/intgm-demarcacion.png" alt="First slide" style="float: left; margin-left:120px; margin-right: 30px;">
                <div class="container">
                  <div class="carousel-caption" style="text-align:right;">
                    <h1 class="text-primary">Demarcaci&oacute;n de v&iacute;as</h1>
                    <p class="text-muted">Todo lo relacionado con pinturas, abrasivos, pistolas</p>
                  </div>
                </div>
              </div>
              <div class="item">
                <img class="second-slide img_thumb_slide" src="/intgm-marcas.png" alt="Second slide" style="float: left; margin-left:120px; margin-right: 30px;">
                <div class="container">
                  <div class="carousel-caption" style="text-align:right;">
                    <h1 class="text-primary">Marcas recomendadas</h1>
                    <p class="text-muted">La reparaci&oacute;n de equipos incluye partes de Graco y Tit&aacute;n</p>
                  </div>
                </div>
              </div>
              <div class="item">
                <img class="third-slide img_thumb_slide" src="/intgm-reparacion.png" alt="Third slide" style="float: left; margin-left:120px; margin-right: 30px;">
                <div class="container">
                  <div class="carousel-caption" style="text-align:right;">
                    <h1 class="text-primary">Reparamos equipos</h1>
                    <p class="text-muted">Ofrecemos servicios de reparaci&oacute;n de equipos Graco y Tit&aacute;n</p>
                  </div>
                </div>
              </div>
            </div>
            <a class="left carousel-control" href="#myProductCarousel" role="button" data-slide="prev">
              <span class="glyphicon glyphicon-chevron-left text-primary" aria-hidden="true"></span>
              <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#myProductCarousel" role="button" data-slide="next">
              <span class="glyphicon glyphicon-chevron-right text-primary" aria-hidden="true"></span>
              <span class="sr-only">Next</span>
            </a>
          </div><!-- /.carousel -->
        <br><br>
        </div>

        <div class="panel panel-primary">
          <div class="panel-heading">
            <h4 class="text-justify"><%= aboutu.descripcion %></h4>
          </div>
        </div>

        <div class="panel panel-primary">
          <div class="panel-body"><br>
            <div class="row text-center">
              <div class="col-sm-4 text-center">
                <center>
                  <div class="img_integral"><br>
                    <%= link_to productos_path do %>
                      <i class="glyphicon glyphicon-briefcase" style="font-size: 40px;"></i>
                    <% end %><br>&nbsp;
                  </div>
                </center>
                <h3 class="text-primary">Productos</h3><br>
                <p class="text-muted">Conozca nuestro portafolio de productos Conozca nuestro portafolio de productosConozca nuestro portafolio de productos</p><br>
              </div>

              <div class="col-sm-4 text-center">
                <center>
                  <div class="img_integral"><br>
                    <%= link_to productos_path do %>
                      <i class="glyphicon glyphicon-wrench" style="font-size: 40px;"></i>
                    <% end %><br>&nbsp;
                  </div>
                </center>
                <h3 class="text-primary">Servicios</h3><br>
                <p class="text-muted">Conozca nuestro portafolio de servicios Conozca nuestro portafolio de serviciosConozca nuestro portafolio de servicios</p><br>
              </div>

              <div class="col-sm-4 text-center">
                <center>
                  <div class="img_integral"><br>
                    <a href="aboutus/1">
                        <i class="glyphicon glyphicon-tower" style="font-size: 40px;"></i>
                    </a><br>&nbsp;
                  </div>
                </center>
                <h3 class="text-primary">Nosotros</h3><br>
                <p class="text-muted">Conozca nuestra empresa, nuestras metas, mision y vision de Integral GM nuestra ubicacionIntegral GM nuestra ubicacion</p><br>
              </div>
            </div>
          <br>
          </div>
        </div>
        <br>
      </div>
    </div>

    <% if !current_user.nil? %> 
      <% if current_user.user_role? %>
        <%= link_to 'Show', aboutu %>
      <% end %>
    <% end %>

  <% end %>
